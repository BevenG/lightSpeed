<h1>Edit Profile</h1>
<%= form_with(model: @user, url: profile_path, local: true) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :profile_picture %>
    <%= form.file_field :profile_picture %>
  </div>
  <div>
    <%= form.submit "Update Profile" %>
  </div>
<% end %>
